            android:theme="@style/HoloActivityTheme" />
        <activity
            android:name="com.example.ddwiki.activities.HoloActivity"
            android:theme="@style/HoloActivityTheme" />
        <activity android:name="com.example.ddwiki.Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
